<th class="fb-table-col-archives">{{tt this "archives"}}</th>
{{#each browserModel.columnsOrder as |columnName|}}
  {{#if (get browserModel (concat "columns." columnName ".isVisible"))}}
    <th
      class={{concat "fb-table-col-" columnName " fb-table-secondary-col hidden-xs"}}
      style={{get browserModel (concat "columnsStyle." columnName)}}
    >
      {{tt this columnName}}
    </th>
  {{/if}}
{{/each}}
{{file-browser/fb-columns-configuration-cell
  browserModel=browserModel
}}