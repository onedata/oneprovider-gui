{{#one-modal
  class="modal-content-overlay modal-content-overlay-right"
  open=open
  backdropClose=false
  keyboard=false
  size="xl"
  onHide=onHide
  as |modal|
}}
  {{#if open}}
    {{#items-select-browser
      modal=modal
      space=space
      constraintSpec=constraintSpec
      onCancel=(action "cancel")
      onSubmit=(action "submit")
    }}
      {{yield}}
    {{/items-select-browser}}
  {{/if}}
{{/one-modal}}
