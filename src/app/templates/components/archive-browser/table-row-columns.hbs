<td
  class="fb-table-col-state fb-table-secondary-col hidden-xs {{fileRowModel.stateColClass}}"
>
  <span class="file-item-text">
    {{fileRowModel.stateText}}
  </span>
</td>
<td class="fb-table-col-incremental fb-table-secondary-col hidden-xs">
  <span
    class="file-item-text {{if fileRowModel.isIncremental "col-status-yes" "col-status-no"}}"
  >
    {{#if fileRowModel.isIncremental}}
      {{#if fileRowModel.baseArchiveNameProxy.isPending}}
        {{tt this "loading"}}
      {{else if fileRowModel.baseArchiveNameProxy.content}}
        <a target={{navigateTarget}} href={{fileRowModel.baseArchiveHref}}>
          {{#truncated-string class="base-archive-name"}}
            {{fileRowModel.baseArchiveNameProxy.content}}
          {{/truncated-string}}
        </a>
      {{else}}
        â€“
      {{/if}}
    {{else}}
      {{tt this "no"}}
    {{/if}}
  </span>
</td>
<td class="fb-table-col-dip fb-table-secondary-col hidden-xs">
  <span
    class="file-item-text {{if fileRowModel.includeDip "col-status-yes" "col-status-no"}}"
  >
    {{tt this (if fileRowModel.includeDip "yes" "no")}}
  </span>
</td>
<td class="fb-table-col-layout fb-table-secondary-col hidden-xs">
  <span class="file-item-text">
    {{fileRowModel.archive.config.layout}}
  </span>
</td>
