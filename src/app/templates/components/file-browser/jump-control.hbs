<div class="form-group form-group-sm search-bar-form-group {{validationClass}}">
  <div class="input-group with-icon-addon">
    <input
      class="form-control jump-input"
      placeholder={{tt this "jumpToPrefix"}}
      type="search"
      onkeydown={{invoke-on-enter (action "performJumpImmediately")}}
      vaule={{jumpInputValue}}
      oninput={{action "changeValue" value="target.value"}}
    >
    <div class="input-group-addon info-input-group-addon">
      {{one-icon icon="browser-info"}}
      {{one-tooltip title=(tt this "tip")}}
    </div>
  </div>
</div>