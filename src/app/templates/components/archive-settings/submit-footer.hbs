{{#bs-button
  class="cancel-archive-creation-btn"
  type="default"
  disabled=isSubmitting
  onClick=(action "close")
}}
  {{tt this "cancel"}}
{{/bs-button}}
{{#one-spin-button
  class="btn btn-primary submit-archive-creation-btn"
  disabled=disabled
  inFlight=isSubmitting
  action=(action "submit")
}}
  {{tt this "create"}}
{{/one-spin-button}}