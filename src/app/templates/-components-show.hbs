{{#perfect-scrollbar-element
  id="content-scroll"
  class="embedded-content scroll-on-top"
  onScroll=(action "scrollOccurred")
  as |scrollbarElement|
}}
  {{component
    model.componentId
    externalData=model.data
    externalActions=model.actions
    containerScrollTop=scrollbarElement.scrollTop
  }}
{{/perfect-scrollbar-element}}
