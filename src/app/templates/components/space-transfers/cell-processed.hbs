{{#if record.transferProgressError}}
  {{error-inline error=record.transferProgressError}}
{{else}}
  <span class="processed-items">{{processedItemsStringified}}</span>
  {{#if (not-eq progressPercent null)}}
    <br />
    <small class="progress-percent text-muted">~{{progressPercent}}%</small>
  {{/if}}
{{/if}}
