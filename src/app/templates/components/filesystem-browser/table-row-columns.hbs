{{#each browserModel.columnsOrder as |columnName|}}
  {{#if (get browserModel (concat "columns." columnName ".isVisible"))}}
    <td class={{concat
        "fb-table-col-" columnName " hidden-xs "
        (if (and (eq columnName "size") file.effFile.isShowProgress) "multiline")
        (if (eq columnName "owner") "multiline")
      }}
      style={{concat "browserModel.columnsStyle." columnName}}
    >
      {{component (concat "filesystem-browser/table-" columnName "-cell")
        browserModel=browserModel
        file=file
        dirStatsServiceState=dirStatsServiceState
        invokeFileAction=(action "invokeFileAction")
      }}
    </td>
  {{/if}}
{{/each}}

{{/if}}
{{#if browserModel.columns.modification.isVisible}}

{{/if}}
{{#if (and browserModel.isOwnerVisible browserModel.columns.owner.isVisible)}}
  <td
    class="fb-table-col-owner multiline hidden-xs"
    style={{browserModel.columnsStyle.owner}}
  >
    <span class="file-item-text">
      {{filesystem-browser/table-cell-owner-info
        owner=owner
        errorReason=errorReasonForOwnerProxy
      }}
    </span>
  </td>
{{/if}} --}}
