{{#each columns as |column|}}
  <td class="workflow-row-cell cell-{{column}}">
    {{#if (eq column "name")}}
      {{#truncated-string}}
        {{atmWorkflowExecutionSummary.name}}
      {{/truncated-string}}
    {{else if (eq column "scheduledAt")}}
      {{moment-format atmWorkflowExecutionSummary.scheduleTime timeFormat "X"}}
    {{else if (eq column "startedAt")}}
      {{moment-format atmWorkflowExecutionSummary.startTime timeFormat "X"}}
    {{else if (eq column "finishedAt")}}
      {{moment-format atmWorkflowExecutionSummary.finishTime timeFormat "X"}}
    {{else if (eq column "status")}}
      {{#if statusIcon}}
        {{#one-icon
          icon=statusIcon
          class=(concat "status-icon status-" atmWorkflowExecutionSummary.status)
        }}
          {{#if statusTooltip}}
            {{one-tooltip title=statusTooltip}}
          {{/if}}
        {{/one-icon}}
      {{/if}}
    {{/if}}
  </td>
{{/each}}
{{yield}}
