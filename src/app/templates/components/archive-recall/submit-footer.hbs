{{#bs-button
  class="cancel-btn"
  type="default"
  disabled=isSubmitting
  onClick=(action onCancel)
}}
  {{tt this "cancel"}}
{{/bs-button}}
{{#one-spin-button
  class="btn btn-primary submit-btn"
  disabled=disabled
  inFlight=isSubmitting
  action=(action onSubmit)
}}
  {{tt this "proceed"}}
{{/one-spin-button}}
{{yield}}