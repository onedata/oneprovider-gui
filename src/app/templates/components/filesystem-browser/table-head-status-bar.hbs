{{filesystem-browser/tag-recalled file=file}}

{{#unless browserModel.previewMode}}
  {{filesystem-browser/file-features
    item=file
    initiallyExpanded=true
    spacePrivileges=browserModel.spacePrivileges
    features=browserModel.fileFeatures
    onInvokeItemAction=(action invokeFileAction)
    onTagHoverChange=(action "changeTagHover")
  }}
{{/unless}}