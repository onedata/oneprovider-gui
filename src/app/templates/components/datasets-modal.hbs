{{#one-modal
  class="datasets-modal"
  open=open
  size="xl"
  backdropClose=false
  keyboard=false
  onHide=(action "onHide")
  as |modal|
}}
  {{file-datasets
    modal=modal
    files=files
    mode=mode
    editPrivilege=editPrivilege
    fileDatasetSummaryLoadError=(if open null)
    getDataUrl=getDataUrl
    getDatasetsUrl=getDatasetsUrl
    close=(action "onHide")
  }}
{{/one-modal}}
