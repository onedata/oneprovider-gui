{{#promise-proxy-container proxy=spaceProxy spinnerBlockClass="absolute-middle-centered"}}
  {{#if hasPermissions}}
    {{space-transfers
      space=spaceProxy.content
      fileId=fileEntityId
      tab=tab
      resetQueryParams=(action "resetQueryParams")
      changeListTab=(action "changeListTab")
      closeFileTab=(action "closeFileTab")
    }}
  {{else}}
    {{errors/no-permissions}}
  {{/if}}
{{/promise-proxy-container}}
