{{#promise-proxy-container
  proxy=shareProxy
  spinnerBlockClass="absolute-middle-centered"
}}
  {{space-shares/share-show
    showSharePath=isPrivate
    showSharePublicUrl=true
    showPublishButton=isPrivate
    share=shareProxy.content
    dirId=dirId
    updateDirId=(action "updateDirId")
    getDataUrl=(action (if isPrivate "getDataUrl" notImplementedIgnore))
    showShareList=(action (if isPrivate "showShareList" notImplementedIgnore))
    reloadShareList=(action (if isPrivate "reloadShareList" notImplementedIgnore))
  }}
{{/promise-proxy-container}}
