{{#one-modal
  id=modalId
  class="datasets-modal modal-content-overlay without-footer with-close-button"
  open=open
  size="xl"
  backdropClose=true
  keyboard=true
  onHide=(action "hide")
  as |modal|
}}
  {{file-datasets
    modal=modal
    files=files
    space=space
    editPrivilege=editPrivilege
    parentModalDialogSelector=parentModalDialogSelector
    fileDatasetSummaryLoadError=(if open null)
    getDataUrl=getDataUrl
    getDatasetsUrl=getDatasetsUrl
    onCloseAllModals=onCloseAllModals
    close=(action "hide")
  }}
  {{!-- FIXME: --}}
  {{log "FIXME: datasetsModal" onCloseAllModals}}
{{/one-modal}}
