<th class="fb-table-col-archives">{{tt this "archives"}}</th>
{{#each browserModel.columnsConfiguration.columnsOrder as |columnName|}}
  {{#if (get browserModel (concat "columnsConfiguration.columns." columnName ".isVisible"))}}
    <th
      class={{concat "fb-table-col-" columnName " fb-table-secondary-col hidden-xs"}}
      style={{get browserModel (concat "columnsConfiguration.columnsStyle." columnName)}}
    >
      {{tt this columnName}}
    </th>
  {{/if}}
{{/each}}
{{columns-configuration-cell
  columnsConfiguration=browserModel.columnsConfiguration
}}